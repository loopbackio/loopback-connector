{
  "name": "loopback-connector",
  "version": "6.0.0-alpha",
  "description": "Building blocks for LoopBack connectors",
  "engines": {
    "node": ">=10"
  },
  "author": "IBM Corp.",
  "keywords": [
    "LoopBack",
    "Connector"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-connector.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "lb-eslint --report-unused-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier:cli": "lb-prettier '**/**.ts'",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --fix",
    "mocha": "lb-mocha . dist/__tests__",
    "test": "lb-nyc npm run mocha",
    "posttest": "npm run lint"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/eslint-config": "^13.0.2",
    "@loopback/testlab": "^5.0.2",
    "async": "^3.2.3",
    "debug": "^4.1.1",
    "msgpack5": "^4.2.0",
    "strong-globalize": "^6.0.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@loopback/build": "^9.0.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "commitlint": "^13.1.0",
    "loopback-datasource-juggler": "^3.32.0"
  }
}
